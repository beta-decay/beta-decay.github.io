<script language="JavaScript" type="text/javascript">
<!-- 
var NN4 = (document.layers) ? true : false;// required for NN4 to reliably capture key eventsif (NN4) document.captureEvents(Event.KEYPRESS)document.onkeypress = function (event) {  checkEnter(event)  return true}//submit the form if the user presses the ENTER keyfunction checkEnter(event){ 		var code = 0		if (NN4) {		code = event.which	} else {		code = window.event.keyCode	}			if (code==13)		document.forms[0].submit()}//===========================================================function setFormFocus() {	if (document.forms[0].Username) {		document.forms[0].Username.focus(); 		document.forms[0].Username.select();		if (document.forms[0].Username.value != "")			{			document.forms[0].Password.focus();			document.forms[0].Password.select();			}  //if Username is prefilled	} //if username exists} //fn setFormFocusfunction setCookie(cookie_name, value, days) 	{	var expires = new Date();	expires.setTime(expires.getTime() + (days * 24 * 60 * 60 * 1000));	document.cookie = cookie_name + "=" + escape(value) + "; expires=" + expires.toGMTString() + "; path=/";	} //fn setCookie	function getCookie(cookie_name) 	{	if (document.cookie.length > 0)		{		cookie_start = document.cookie.indexOf(cookie_name + "=")		if (cookie_start != -1)	  		{ 			cookie_start = cookie_start + cookie_name.length + 1 			cookie_end = document.cookie.indexOf(";", cookie_start)			if (cookie_end == -1) 				{				cookie_end = document.cookie.length				} //if cookie_end			return unescape(document.cookie.substring(cookie_start, cookie_end))			} //if cookie_start != -1 		} //if cookie length > 0		return ""; 	//when found nothing return blank	} //fn getCookie
// -->
</script>
